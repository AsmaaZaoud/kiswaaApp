{
  "expo": {
    "name": "Kiswa",
    "slug": "Kiswa",
    "plugins": ["expo-image-picker"],
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.7.1",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/Fatima/splashPage.gif",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.kiswa23.Kiswa",
      "versionCode": 1,
      "adaptiveIcon": {
        "forgroundImage": "./assets/Fatima/Logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "description": "Kiswa is a capstone project 2023",
    "extra": {
      "eas": {
        "projectId": "41f9f18d-1662-47a7-874f-d2a8eae80c9d"
      }
    }
  }
}
